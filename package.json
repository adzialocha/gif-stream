{
  "name": "gif-stream",
  "version": "0.1.0",
  "description": "Take pictures from peoples cameras in intervals, make a .gif, upload it to S3, make a massive collage out of this",
  "main": "dist/gif-stream.js",
  "scripts": {
    "lint": "eslint build.js src/** test/**; exit 0",
    "test": "mocha test/** --compilers js:babel-register; exit 0",
    "test:watch": "mocha test/** --compilers js:babel-register --reporter min --watch test/** src/**",
    "watch": "watch 'npm run build' ./src",
    "serve": "static-server",
    "build": "rollem -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/adzialocha/gif-stream.git"
  },
  "author": "Andreas Dzialocha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adzialocha/gif-stream/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/adzialocha/gif-stream#readme",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rollem": "^1.8.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "static-server": "^2.0.4",
    "watch": "^1.0.2"
  }
}
